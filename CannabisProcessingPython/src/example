gsutil -m cp gs://cannabis-3k/kannapedia/RSP11055_1.fastq . &&
gsutil -m cp gs://cannabis-3k/kannapedia/RSP11055_2.fastq . &&
minimap2 -ax sr reference/AGQN03/AGQN03.fa RSP11055_1.fastq RSP11055_2.fastq -R '@RG\tID:RSP11055\tPL:ILLUMINA\tPU:NONE\tSM:RSP11055' | samtools view -bh - | samtools sort -m 2G -@ 4 > RSP11055_AGQN03.sorted.bam &&
gsutil -m mv RSP11055_AGQN03.sorted.bam gs://cannabis-3k-results/manual/bam/RSP11055_AGQN03.sorted.bam &&
minimap2 -ax sr reference/LKUA01/LKUA01.fa RSP11055_1.fastq RSP11055_2.fastq -R '@RG\tID:RSP11055\tPL:ILLUMINA\tPU:NONE\tSM:RSP11055' | samtools view -bh - | samtools sort -m 2G -@ 4 > RSP11055_LKUA01.sorted.bam &&
